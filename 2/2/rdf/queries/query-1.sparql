PREFIX ex:   <http://example.org/vocabulary/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>

SELECT ?actor (COUNT(DISTINCT ?director) AS ?directorCount)
WHERE {
  ?a a ex:Actor ;
     foaf:name ?actor ;
     ex:actedIn ?movie .
  ?movie ex:directedBy ?director .
}
GROUP BY ?actor
HAVING (COUNT(DISTINCT ?director) > 1)
ORDER BY DESC(COUNT(DISTINCT ?director))
