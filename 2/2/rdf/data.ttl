### COUNTRIES

data:country/us a ex:Country;
    rdfs:label "United States"@en;
    ex:isoCode "US".

data:country/uk a ex: Country ;
    rdfs:label "United Kingdom"@en;
    ex:isoCode "GB".

data:country/fr a ex:Country;
    rdfs:label "France"@en;
    ex:isoCode "FR".

### STUDIOS

data:studio/warner-bros a ex:Studio;
    foaf:name "Warner Bros. Pictures"@en;
    ex:foundedYear "1923"^^xsd:gYear;
    ex:moviesProduced 12500;
    ex:basedIn data:country/us.

data:studio/universal a ex:Studio;
    foaf:name "Universal Pictures"@en;
    ex:foundedYear "1912"^^xsd:gYear;
    ex:moviesProduced 400;
    ex:basedIn data:country/us.

data:studio/legendary a ex:Studio;
    foaf:name "Legendary Pictures"@en;
    ex:foundedYear "2000"^^xsd:gYear;
    ex:moviesProduced 70;
    ex:basedIn data:country/us.


data:studio/pinewood a ex:Studio;
    foaf:name "Pinewood"@en;
    ex:foundedYear "1936"^^xsd:gYear;
    ex:moviesProduced 1500;
    ex:basedIn data:country/uk.


### CINEMAS

